DECL
float [-1000.000000, 1000.000000] cb1;
float [-1000.000000, 1000.000000] cb2;
float [0, 1.000000] xi1;
float [0, 1.000000] t;
int [-1000,1000] n;

INIT
n = 0;
cb1 = 0;
cb2 = 10.000000;

TRANS
n' = n+1;
cb1' = cb1 + cb2;cb2' = -(cb1 + cb2*xi1)^3;

TARGET
n>500 && ((20 > cb1 + cb2*t)) or((cb1 + cb2*t > -20));

